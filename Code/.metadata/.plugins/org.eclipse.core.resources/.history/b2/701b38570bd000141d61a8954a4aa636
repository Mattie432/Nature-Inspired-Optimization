package Assessment2;
import org.moeaframework.core.Solution;
import org.moeaframework.core.variable.EncodingUtils;
import org.moeaframework.core.variable.RealVariable;
import org.moeaframework.problem.AbstractProblem;


public class PinnedPinnedSandwichBeamProblem extends AbstractProblem {

	private double [][] pEc = new double[3][3]; //density p, young constant E, cost c for each material

	public PinnedPinnedSandwichBeamProblem() {
		super(4,2);
		
		//material 1
		pEc[0][0] = 100.0;
		pEc[0][1] = 1.60E+09;
		pEc[0][2] = 500.0;
		//material 2
		pEc[1][0] = 2770.0;
		pEc[1][1] = 7.00E+10;
		pEc[1][2] = 1500.0;
		//material 2
		pEc[1][0] = 7780.0;
		pEc[1][1] =  2.00E+11;
		pEc[1][2] = 800.0;
	}
	
	
	@Override
	public void evaluate(Solution solution) {
		// TODO Auto-generated method stub
		double[] x = EncodingUtils.getReal(solution);
		double f1 = 0.0;
		double f2 = 0.0;	
		//double sqrt2 = Math.sqrt(2);
		
		double b = x[0];
		double L = x[1];
		double u = x[2];
		
		double EI = 2*b / 3 * (pEc[0][1]*Math.pow(x[3],3) + pEc[1][1]*(Math.pow(x[4],3)-Math.pow(x[3],3)));
		
		//f1 = L*(2*x[0] + sqrt2*x[1] + Math.sqrt(x[2]) + x[3]);
		f1 = Math.PI / 2*Math.pow(L,2) * (Math.pow(EI / u,0.5));
		//f2 = F*L/E * (2/x[0] + 2*sqrt2/x[1] -  2*sqrt2/x[2] + 2/x[3]);
		
		solution.setObjective(0, f1);
		//solution.setObjective(1, f2);

	}

	@Override
	public Solution newSolution() {
		// TODO Auto-generated method stub
		Solution solution = new Solution(numberOfVariables, numberOfObjectives);
				
		solution.setVariable(0,  new RealVariable(0.3, 0.6));       // d_1
		solution.setVariable(1,  new RealVariable(0.01, 0.58));     // d_2
		solution.setVariable(2,  new RealVariable(0.01, 0.58));     // d_3
		
		solution.setVariable(3,  new RealVariable(0.3, 0.6));       // b (beam width)
		solution.setVariable(4,  new RealVariable(0.3, 0.55));       // L (beam length)
		
		
	
		
		return solution;
	}
	
	
	
}
